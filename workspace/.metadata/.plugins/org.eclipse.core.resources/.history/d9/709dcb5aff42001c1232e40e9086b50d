package java04.application.Membership;
// 메소드 만들기
// public [반환자료형] [메소드명](입력할 데이터 들)
// 어떠한 메소드가(여기서는 main) static으로 선언되어 있고
// 내부에서 외부의 메소드를 호출한다면 그 메소드는 static으로 정의되어 있어야 한다.
public class Ex04 {

	public static void main(String[] args) {
		int sum;
//		라면을 사온다.
		sumf(1,2);
//		재료를 손질한다.
		menu();
		
//		물을 끓인다.
//		1. 집 밖으로
//		2. 우리집은 아파트 50층 그래서 엘베 기다림
//		2. 우리집은 아파트 50층 그래서 엘베 기다림
//		2. 우리집은 아파트 50층 그래서 엘베 기다림
//		2. 우리집은 아파트 50층 그래서 엘베 기다림
//		2. 우리집은 아파트 50층 그래서 엘베 기다림
	}

	private static void menu() {
		// TODO Auto-generated method stub
		System.out.println("1. 회원가입");
		System.out.println("2. 로그인");
		System.out.println("3. 종료");
	}

	private static int sumf(int i, int j) {
		// TODO Auto-generated method stub
		return 0;
	}
	
}
